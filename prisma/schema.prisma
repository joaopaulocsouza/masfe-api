generator client {
  provider = "prisma-client-js"
}

datasource db {
  url      = "postgresql://postgres:Cl0d0V1l10@@localhost:5432/MASFe"
  provider = "postgresql"
}

model User {
  id         String    @id @default(uuid())
  name       String
  email      String    @unique
  password   String
  gender     String?
  occupation String?
  endereco   String?
  birthday   DateTime?

  @@map("users")
}
